<h1>Varients#new</h1>
<p>Find me in app/views/varients/new.html.erb</p>

<h1>Color and Material</h1>

<%= form_for [@style, @varient] do |f| %>

	<%= f.label :image %>
	<%= f.file_field :picture %>

	<%= f.label :color %>
	<%= f.text_field :color %>

	<%= f.label :material %>
	<%= f.text_field :material %>



	<%= f.submit %>
<% end %>
